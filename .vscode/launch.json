{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "index.html",
      "request": "launch",
      "type": "chrome",
      "url": "http://localhost:5500/index.html",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "node: current file",
      "program": "${file}",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "console": "integratedTerminal"
    }
  ]
}
